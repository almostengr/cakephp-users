{
    "name": "almostengr/cakephp-users",
    "description": "Users plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0.0",        
        "cakephp/authentication": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "CakephpUsers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakephpUsers\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
